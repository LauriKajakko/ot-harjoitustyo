<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.chart.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.text.*?>

<AnchorPane prefHeight="400.0" prefWidth="600.0" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="shiftplanner.EmployeeInfoController">
   <children>
      <HBox layoutX="-1.0" prefHeight="400.0" prefWidth="1000.0">
         <children>
            <VBox prefHeight="400.0" prefWidth="223.0">
               <children>
                  <Button fx:id="goBack" mnemonicParsing="false" text="Go Back">
                     <VBox.margin>
                        <Insets left="5.0" top="5.0" />
                     </VBox.margin>
                  </Button>
                  <HBox prefHeight="26.0" prefWidth="200.0">
                     <children>
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="First name:">
                           <HBox.margin>
                              <Insets left="10.0" right="10.0" />
                           </HBox.margin>
                        </Text>
                        <Text fx:id="employeeName" strokeType="OUTSIDE" strokeWidth="0.0" text="Employee Name" />
                     </children>
                     <VBox.margin>
                        <Insets top="40.0" />
                     </VBox.margin>
                  </HBox>
                  <HBox prefHeight="27.0" prefWidth="177.0">
                     <children>
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Last name:">
                           <HBox.margin>
                              <Insets left="10.0" right="10.0" />
                           </HBox.margin></Text>
                        <Text fx:id="employeeLastname" strokeType="OUTSIDE" strokeWidth="0.0" text="Employee lastname" />
                     </children>
                  </HBox>
                  <HBox prefHeight="37.0" prefWidth="177.0">
                     <children>
                        <Text strokeType="OUTSIDE" strokeWidth="0.0" text="Role:">
                           <HBox.margin>
                              <Insets left="10.0" right="44.0" />
                           </HBox.margin></Text>
                        <Text fx:id="employeeRole" strokeType="OUTSIDE" strokeWidth="0.0" text="Emloyee role" />
                     </children>
                  </HBox>
               </children>
            </VBox>
            <BarChart fx:id="hourChart" prefHeight="400.0" prefWidth="843.0" title="Hours">
              <xAxis>
                <CategoryAxis side="BOTTOM" />
              </xAxis>
              <yAxis>
                <NumberAxis side="LEFT" />
              </yAxis>
               <HBox.margin>
                  <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
               </HBox.margin>
            </BarChart>
         </children>
      </HBox>
   </children>
</AnchorPane>
